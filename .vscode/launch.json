{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/coffee-script/bin/coffee",
			"stopOnEntry": false,
			"args": ["node_modules/hubot/bin/hubot", "--name", "donkeybot", "-l", "db"],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
                "FFMPEG_PATH": "",
                "HUBOT_MUMBLE_URL": "",
                "HUBOT_MUMBLE_PASSWORD": ""
			},
			"externalConsole": true,
			"sourceMaps": true,
			"outDir": null
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}