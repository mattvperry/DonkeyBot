{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/tsbot/bin/tsbot",
			"stopOnEntry": false,
			"args": ["--name", "donkeybot", "-l", "db"],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy",
                "--harmony_default_parameters",
                "--harmony_rest_parameters",
                "--harmony_destructuring"
			],
			"env": {
				"NODE_ENV": "development",
                "FFMPEG_PATH": "",
                "HUBOT_MUMBLE_URL": "",
                "HUBOT_MUMBLE_PASSWORD": "",
                "HUBOT_GOOGLE_CSE_KEY": "",
                "HUBOT_WOW_API_KEY": ""
			},
			"externalConsole": true,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/scripts"
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}